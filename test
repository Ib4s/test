#include <stdio.h>

int main()
{
printf("Oi, Eu sou Eduardo aluno de Engenharia De Computação do Ceub.\n");
printf("O meu número da sorte é: %d.\n",7);
printf("O valor de pi é %f.\n",3.1415926);
printf("O valor de pi com 2 casas é:%.2f.\n",3.1415926);
printf("O menor buraco negro da Via Láctea tem %.2e de Km de diâmetro.\n",35000000.0);
printf("A primeira letra do meu nome é:%c.\n",'E');
printf("O sobrenome da minha família é:%s.\n","De Regueiro Costa");
printf("O planeta Terra tem %x anos de idade em hexadecimal.\n",4500000000);
printf("Eu tenho %o anos de idade em octal, enquanto meus irmão têm %o anos de idade em octal.",19,15);

return 0;
}
